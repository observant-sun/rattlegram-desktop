<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.github.observant_sun.rattlegram.controller.SpectrumAnalyzerWindowController"
            fx:id="rootAnchorPane" prefHeight="276" prefWidth="380">
    <VBox AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10"
          fx:id="vBox"
          maxWidth="360" minWidth="360"
          maxHeight="256" minHeight="256" >
        <ImageView fx:id="spectrumImageView" VBox.vgrow="ALWAYS"/>
        <ImageView fx:id="spectrogramImageView" VBox.vgrow="ALWAYS"/>
    </VBox>
</AnchorPane>
